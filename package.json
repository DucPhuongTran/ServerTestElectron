{
  "name": "adeptwebviewer",
  "productName": "Adept",
  "version": "1.0.0",
  "description": "Application to View Local files",
  "main": "main.js",
  "scripts": {
    "start": "tsc && electron main.js",
    "electron": "tsc && electron main.js --inspect=9009 --devTools=true --launcher",
    "package": "tsc && electron-packager ./ AdeptViewer --overwrite --ignore=AdeptViewerLauncher --ignore=AdeptViewerService --ignore=CleanupTool --ignore=ConfigFiles --ignore=ViewerCleanup --icon=viewer-icon.ico --Win32MetadataOptions.FileDescription=\"Adept -- Viewer\"",
    "setperm": "icacls adeptwebviewer-win32-x64 /grant Everyone:F /T",
    "copyFiles": "tsc copy.ts && node ./copy.js"
  },
  "author": "Synergis Technologies, LLC",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-packager": "^16.0.0"
  },
  "dependencies": {
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.10.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "electron-browser-storage": "^1.0.6",
    "electron-dl": "^3.5.0",
    "electron-log": "^4.3.1",
    "express": "^4.17.1",
    "g": "^2.0.1",
    "http-proxy-middleware": "^2.0.0",
    "mongodb": "^3.6.2",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0",
    "node-windows": "^1.0.0-beta.5",
    "ps-list": "^7.2.0",
    "rxjs": "^6.6.3",
    "socket.io": "^4.4.1",
    "winreg": "^1.2.4"
  }
}
